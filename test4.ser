fun fib(x : int) -> int {
  if (x <= 1) {
    return x;
  } else {
  return fib(x - 1) + fib(x - 2);
  }
}

const tab_size = 50;
const tab : int[50];

fun fast_fib(x : int) -> int {
  if (x <= 1) {
    return x;
  } else {
    if (tab[x] == 0) {
      tab[x] = fast_fib(x - 1) + fast_fib(x - 2);
    }
    return tab[x];
  }
}

